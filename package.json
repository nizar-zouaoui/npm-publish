{
  "name": "nizar-npm-publish-test",
  "description": "this is a temp project to test exports after build",
  "version": "0.0.10",
  "repository": {
    "type": "git",
    "url": "https://github.com/nizar-zouaoui/npm-publish"
  },
  "main": "build/index.js",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "exports": {
    ".": {
      "import": "./build/index.mjs",
      "require": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./*": {
      "import": "./build/*.mjs",
      "require": "./build/*.js",
      "types": "./build/*.d.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf build",
    "build": "yarn clean && yarn build:esm && yarn build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json --outDir build",
    "build:cjs": "tsc -p tsconfig.cjs.json  --outDir build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "launch": "npx ts-node index"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/rimraf": "^4.0.5",
    "dynamodb-onetable": "^2.7.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1"
  }
}
